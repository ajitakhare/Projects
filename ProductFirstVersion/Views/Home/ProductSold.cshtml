@model IEnumerable<ProductFirstVersion.Models.customerproductnameview>

@{
    ViewBag.Title = "ProductSold";
}

<h2>ProductSold</h2>

<p>
    @Html.ActionLink("Create New", "CreateProductSold")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ProductSoldId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProductName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CustomerName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ProductSoldId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProductName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CustomerName)
        </td>
     
    </tr>
}

</table>
